<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RScript xmlns="http://www.fao.org/rScript">
    <Name>Impute Production</Name>
    <Description>Impute missing production data using the ensemble model.  This module works on just the commodities/countries open in the current session.</Description>
    <Category>Imputation</Category>
    <Authors>
        <Author>
            <Name>Josh Browning</Name>
            <Mail>joshua.browning@fao.org</Mail>
        </Author>
    </Authors>
    <Params>
        <Param xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="Number">
            <Code>startYear</Code>
            <Label>First year</Label>
            <Description>The start of the computation period</Description>
            <Compulsory>true</Compulsory>
            <Min>1961</Min>
            <Max>2050</Max>
        </Param>
        <Param xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="Number">
            <Code>endYear</Code>
            <Label>Last year</Label>
            <Description>The end of the computation period</Description>
            <Compulsory>true</Compulsory>
            <Min>1961</Min>
            <Max>2050</Max>
        </Param>
    </Params>
    <Datasets>
        <MainDataset>
            <Code>aproduction</Code>
            <SelectableDimensions>
                <SelectableDimension compulsory="true">geographicAreaM49</SelectableDimension>
                <SelectableDimension compulsory="true">measuredItemCPC</SelectableDimension>
                <SelectableDimension compulsory="true">measuredElement</SelectableDimension>
                <SelectableDimension compulsory="true">timePointYears</SelectableDimension>
            </SelectableDimensions>
        </MainDataset>
    </Datasets>
    <InteractiveValidation>false</InteractiveValidation>
    <Background>false</Background>
</RScript>
